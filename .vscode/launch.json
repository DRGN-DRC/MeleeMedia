	"version": "0.2.0",
	"configurations": [
		{
			"name": "Build and Test Loop",
			"type": "clr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/MeleeMedia/bin/Debug/net461/MeleeMedia.exe",
